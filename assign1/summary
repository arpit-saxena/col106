Position:
	value()
	after()

LinkedList<T>:
	add(T);
	Iterator<Position<T>> positions();
	int count();

Entity: // Hostel, Dept, or Course
	String name();
	Iterator<Student> studentList();

GradeInfo:
	enum LetterGrade;
	static int gradepoint(LetterGrade);

CourseGrade: // Tuple of course and grade
	String coursetitle();
	String coursenum();
	GradeInfo grade() 

Student:
	String name();
	String entryNo();
	String hostel();
	String department();
	String completedCredits();
	String cgpa();
	Iterator<CourseGrade> courseList();

Assignment1:
	static main(String[]); // Command line params: 
		// student record fle, course file, student query file.
